{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "prettier",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["prettier", "@typescript-eslint"],
    "rules": {
        "prettier/prettier": "warn",
        "no-console": "warn"
    }
}
